{
  "extends": "airbnb",
  "env": {
    "node": true
  },
  "overrides": [
    {
      "files": [ "database/**/*.js" ],
      "rules": {
        "object-shorthand": 0,
        "func-names": 0,
        "strict": 0,
        "comma-dangle": 0,
        "eol-last": 0,
        "no-unused-vars": 0,
        "space-before-function-paren": 0
      }
    }
  ],
  "rules": {
    "max-len": 0,
    "no-console": 0,
    "arrow-parens": 0,
    "class-methods-use-this": 0,
    "lines-around-directive": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "no-unused-vars": [
      2,
      {
        "argsIgnorePattern": "^_"
      }
    ]
  }
}